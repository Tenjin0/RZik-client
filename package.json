{
  "name": "mobx-example",
  "version": "1.0.0",
  "description": "Example for MobX + React. Includes hot code reloading in development mode and minification in production mode.  Also includes a demonstration of how to hook up a CMS API and do database calls and loading states.",
  "scripts": {
    "start": "node server.js",
    "dash": " NODE_ENV=development webpack-dashboard -c cyan -- node server.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "react-hot-loader": "3.0.0-beta.2",
    "react-router": "^4.1.1",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "svg-sprite-loader": "^3.0.5",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "cosmicjs": "^2.39.91",
    "eslint": "^3.19.0",
    "express": "^4.15.3",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "id3js": "^1.1.3",
    "jquery": "^3.2.1",
    "material-ui": "^0.18.1",
    "mobx": "^3.1.11",
    "mobx-react": "^4.2.1",
    "mobx-react-devtools": "^4.2.13",
    "pug": "^2.0.0-rc.1",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "shorti": "^1.2.6",
    "slug": "^0.9.1"
  }
}
